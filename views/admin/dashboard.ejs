<h3>Dashboard</h3>
<div>
    <a href="/add-post">add new</a>
</div>

<ul>
    <% data.forEach(post => { %>
        <li>
            <a href="/post/<%= post._id %>">
                <%= post.title %>
            </a>
            <div>
                <a href="/edit-post/<%= post._id %>">edit</a>

                <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="delete">
                </form>
            </div>
        </li>
    <% }) %>
</ul>